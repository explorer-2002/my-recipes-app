{
  "expo": {
    "name": "mobileApp",
    "jsEngine": "hermes",
    "slug": "mobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/i1.png",
    "scheme": "mobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.sanyamj924.mobileApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/i1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/i1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0000",
          "dark": {
            "backgroundColor": "#0000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ea9d6bdd-5555-4277-9b34-73634910d219"
      }
    }
  }
}
